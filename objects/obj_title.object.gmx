            <string>pulse += 0.01;
if (pulse &gt; 1)
    pulse = 0;
    
if (keyboard_check_pressed(vk_up)) {
    audio_play_sound(snd_select, 0, false);
    option--;
}
if (keyboard_check_pressed(vk_down)) {
    audio_play_sound(snd_select, 0, false);
    option++;
